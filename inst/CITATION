citHeader("To cite hbrfit in publications, please use:")

bibentry(bibtype = "Article",
  author = personList(as.person("Jeff T. Terpstra"), as.person("Joseph W. McKean")),
  title = "Rank-Based Analysis of Linear Models Using R,",
  journal = "Journal of Statistical Software",
  year = "2005",
  volume = "14",
  number = "7",
  pages = "1--26",
  textVersion = 
  paste(
"Terpstra, J.T., and McKean. J.W., (2005)",
"Rank-based analysis of linear models using R.",
"Journal of Statistical Software, 14, 1-26."
)
)

bibentry(bibtype = "Book",
  author = c(person(c("John"), "Kloke"), person(c("Joseph", "W."), "McKean
")),
  title = "Nonparametric Statistical Methods Using R",
  year = "2014",
  publisher="Chapman-Hall",
  address="Boca Raton, FL",
  textVersion = 
  paste("Kloke, J. and McKean, J.W. (2014),",
        "Nonparametric Statistical Methods Using R", 
        "Boca Raton, FL: Chapman-Hall."
       )
)


